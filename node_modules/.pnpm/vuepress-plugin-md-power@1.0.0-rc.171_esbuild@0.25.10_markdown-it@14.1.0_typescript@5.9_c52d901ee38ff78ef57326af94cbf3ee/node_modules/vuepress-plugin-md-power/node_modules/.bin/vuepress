#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/桌面/User/github/it4keth2ee/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.7.2_typescript@5_de53b39717bffaeb4f1803cb33eb564d/node_modules/vuepress/bin/node_modules:/home/user/桌面/User/github/it4keth2ee/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.7.2_typescript@5_de53b39717bffaeb4f1803cb33eb564d/node_modules/vuepress/node_modules:/home/user/桌面/User/github/it4keth2ee/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.7.2_typescript@5_de53b39717bffaeb4f1803cb33eb564d/node_modules:/home/user/桌面/User/github/it4keth2ee/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/桌面/User/github/it4keth2ee/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.7.2_typescript@5_de53b39717bffaeb4f1803cb33eb564d/node_modules/vuepress/bin/node_modules:/home/user/桌面/User/github/it4keth2ee/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.7.2_typescript@5_de53b39717bffaeb4f1803cb33eb564d/node_modules/vuepress/node_modules:/home/user/桌面/User/github/it4keth2ee/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.7.2_typescript@5_de53b39717bffaeb4f1803cb33eb564d/node_modules:/home/user/桌面/User/github/it4keth2ee/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.7.2_typescript@5_de53b39717bffaeb4f1803cb33eb564d/node_modules/vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../../../../vuepress@2.0.0-rc.24_@vuepress+bundler-vite@2.0.0-rc.24_@types+node@24.7.2_typescript@5_de53b39717bffaeb4f1803cb33eb564d/node_modules/vuepress/bin/vuepress.js" "$@"
fi
